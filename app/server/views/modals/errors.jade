
.modal-error.modal.hide.fade
	div.modal-header
		button.close(data-dismiss='modal') x
		h3=__("Error")
	div.modal-body
		p.subheading=__("It looks like there was a problem")
		ul
	div.modal-footer
		button(data-dismiss="modal").cancel.btn=__("Cancel")
		button(data-dismiss="modal")#ok.btn.btn-danger=__("OK")
		button(data-dismiss="modal")#force.btn.btn-confirm=__("SAVE IT ANYWAY")