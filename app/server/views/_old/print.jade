	body
		- if (locals.udata)
			div.navbar.navbar-inverse.navbar-fixed-top
				div.navbar-inner
					div.container
						ul#global-nav.nav.pull-left
							li
								a(href="/").brand Control Panel
						a(href="/logout")#btn-logout.btn.btn-primary.pull-right
							i.icon-lock.icon-white
							| Sign Out
.container
	div.row.noprint
		h1.span6=__("Print")
		.span6
			.pull-right
				br
				input(type="button", value=__("PRINT"), onclick="window.print()").btn.btn-primary
		.span12
			hr.clearfix
.print_container
	.cntPadd!= body
div.noprint!="&nbsp";
