	head
		title= title
		link(rel="stylesheet", href="/lib/bootstrap.min.css")
		link(rel="stylesheet", href="/css/style.css")
		link(rel="stylesheet", href="/css/print_templates.css")
		meta(name="viewport", content="width=device-width, initial-scale=1.0")
		
